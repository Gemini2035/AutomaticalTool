{
  "name": "automation_tool",
  "version": "1.0.0",
  "type": "module",
  "description": "Automation tool for commerce search",
  "main": "./src/main.ts",
  "author": "Gemini2035",
  "bin": "./src/main.ts",
  "scripts": {
    "dev": "tsup src/main.ts --format esm --out-dir packages --dts && node ./packages/main.js",
    "build": "tsup src/main.ts --format esm --out-dir packages && pkg ./packages/main.js --out-path=AutomationTool --targets node18-win-arm64",
    "build:dev": "tsup src --format esm --out-dir packages && pkg ./packages/main.js --out-path=dist --debug --targets node18-win-arm64 && rm -rf packages"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.4",
    "dayjs": "^1.11.13",
    "fuse.js": "^7.0.0",
    "puppeteer": "^23.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@swc/core": "^1.7.14",
    "@types/node": "^22.5.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
